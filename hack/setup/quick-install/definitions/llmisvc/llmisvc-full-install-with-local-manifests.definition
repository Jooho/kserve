DESCRIPTION: Install LLM InferenceService dependencies using helm and kserve using kustomize 
METHOD: helm

TOOLS:
  - helm
  - kustomize
  - yq

COMPONENTS:
  - name: external-lb
  - name: cert-manager
  - name: gateway-api-crd
  - name: envoy-gateway
  - name: envoy-ai-gateway
  - name: lws-operator
  - name: kserve-gateway
  - name: kserve-kustomize
    env:
      LLMISVC: "true"
